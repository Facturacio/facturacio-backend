<?xml version="1.0" encoding="UTF-8"?>
<fe:Facturae xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
    xmlns:fe="http://www.facturae.es/Facturae/2014/v3.2.1/Facturae">
    <FileHeader>
        <SchemaVersion>3.2.1</SchemaVersion>
        <Modality>I</Modality>
        <InvoiceIssuerType>EM</InvoiceIssuerType>
        <Batch>
            <BatchIdentifier>#Número_d'identificació_fiscal_Personal#Número_Factura#Sèrie_Factura</BatchIdentifier>
            <InvoicesCount>1</InvoicesCount>
            <TotalInvoicesAmount>
                <TotalAmount>#Total_Pagament</TotalAmount>
            </TotalInvoicesAmount>
            <TotalOutstandingAmount>
                <TotalAmount>#Total_realitzar</TotalAmount>
            </TotalOutstandingAmount>
            <TotalExecutableAmount>
                <TotalAmount>#Total_realitzar</TotalAmount>
            </TotalExecutableAmount>
            <InvoiceCurrencyCode>EUR</InvoiceCurrencyCode>
        </Batch>
    </FileHeader>
    <Parties>
        <SellerParty>
            #dades_personals
        </SellerParty>
        <BuyerParty>
            #dades_client
        </BuyerParty>
    </Parties>
    <Invoices>
        <Invoice>
            <InvoiceHeader>
                <InvoiceNumber>#Número_Factura</InvoiceNumber>
                <InvoiceSeriesCode>#Sèrie_Factura</InvoiceSeriesCode>
                <InvoiceDocumentType>FC</InvoiceDocumentType>
                <InvoiceClass>OO</InvoiceClass>
            </InvoiceHeader>
            <InvoiceIssueData>
                <IssueDate>#Data_Factura</IssueDate>
                <InvoiceCurrencyCode>EUR</InvoiceCurrencyCode>
                <TaxCurrencyCode>EUR</TaxCurrencyCode>
                <LanguageName>ca</LanguageName>
            </InvoiceIssueData>
            #impostos_totals
            <InvoiceTotals>
                <TotalGrossAmount>#Subtotal_Pagament</TotalGrossAmount>
                <TotalGeneralDiscounts>0.0</TotalGeneralDiscounts>
                <TotalGeneralSurcharges>0.0</TotalGeneralSurcharges>
                <TotalGrossAmountBeforeTaxes>#Subtotal_Pagament</TotalGrossAmountBeforeTaxes>
                <TotalTaxOutputs>#Total_Imposts</TotalTaxOutputs>
                <TotalTaxesWithheld>0.0</TotalTaxesWithheld>
                <InvoiceTotal>#Total_Pagament</InvoiceTotal>
                #pagaments_anticipats
                <TotalOutstandingAmount>#Total_realitzar</TotalOutstandingAmount>
                #total_anticipats
                <TotalExecutableAmount>#Total_realitzar</TotalExecutableAmount>
                <TotalReimbursableExpenses>0.0</TotalReimbursableExpenses>
            </InvoiceTotals>
            #dades_linies
            <PaymentDetails>
                <Installment>
                    <InstallmentDueDate>#Data_termini_Pagament</InstallmentDueDate>
                    <InstallmentAmount>#Total_realitzar</InstallmentAmount>
                    #Mitjà_de_pagament_Pagament
                </Installment>
            </PaymentDetails>
            #dades_literals
        </Invoice>
    </Invoices>
</fe:Facturae>